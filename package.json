{
	"name": "turing-tool",
	"displayName": "turing_tool",
	"description": "Turing  Platform provides various program tools",
	"publisher": "TURING-TEAM-DEV",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:turing.tool",
		"onView:t-view"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "turing.tool",
				"title": "图聆工具栏"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "turing-id",
					"title": "图聆小工具",
					"icon": "source/icon.png"
				}
			]
		},
		"views": {
			"turing-id": [
				{
					"id": "t-view",
					"name": "工具列表"
				}
			]
		}
	},
	"repository": {
        "type": "git",
        "url": "https://github.com/wanganjuan/turing_tool_one.git"
    },
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}